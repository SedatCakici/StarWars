	        this.robotType = robotType;
	    }
	    
	    public void tellItLikeItIs() {
	    	switch(robotType){
	    	case R2D2:
	    		break;
	    	case C3PO:
	    		break;
			default:
				break;
	    	
	    	}
	    	
	    }
}

package deHsMannheimSS17Tpe;

import static org.junit.Assert.*;

import org.junit.Test;

public class RobotFactoryTest {

	@Test
	public void test() {
		RobotFactory rf = new RobotFactory();
		Robot output = rf.createRobot("R2D2");
		assertEquals("R2D2", output);
		
		Robot output = rf.createRobot("C3PO");
		assertEquals("C3PO", output);
		
		Robot output = rf.createRobot("Bender");
		assertEquals("Bender", output);
		
	}

}
